          {
            "name": "detection_model",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "default": "db_resnet50",
              "title": "Detection Model"
            },
            "description": "Detection architecture to use. See the models page for available options."
          },
          {
            "name": "recognition_model",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "default": "crnn_vgg16_bn",
