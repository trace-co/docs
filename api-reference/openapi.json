            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": true,
              "title": "Preserve Aspect Ratio"
            },
            "description": "Pad the image to preserve its aspect ratio before feeding it to the model."
          },
          {
            "name": "detect_orientation",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Detect Orientation"
            },
            "description": "Detect and report the page orientation angle in the response."
          },
          {
            "name": "detect_language",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Detect Language"
            },
            "description": "Detect and report the page language in the response."
          },
          {
            "name": "symmetric_padding",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": true,
              "title": "Symmetric Padding"
            },
            "description": "Pad symmetrically (centered) rather than bottom-right only."
          },
          {
            "name": "straighten_pages",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Straighten Pages"
            },
            "description": "Automatically rotate pages to correct detected skew before recognition."
          },
          {
            "name": "detection_batch_size",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 2,
              "title": "Detection Batch Size"
            },
            "description": "Number of pages processed in parallel during detection. Increase for multi-page PDFs if you have enough memory."
          },
          {
            "name": "recognition_batch_size",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 128,
              "title": "Recognition Batch Size"
            },
            "description": "Number of text crops processed in parallel during recognition. Decrease if you run into memory issues on large batches."
          },
          {
            "name": "disable_page_orientation",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Disable Page Orientation"
            },
            "description": "Skip page orientation classification entirely."
          },
          {
            "name": "disable_text_orientation",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Disable Text Orientation"
            },
            "description": "Skip text crop orientation classification."
          },
          {
            "name": "binary_threshold",
            "in": "query",
            "required": false,
            "schema": {
              "type": "number",
              "default": 0.1,
              "title": "Binary Threshold"
            },
            "description": "Pixel-level threshold for the detection segmentation heatmap. Lower values detect more text but may introduce false positives."
          },
          {
            "name": "box_threshold",
            "in": "query",
            "required": false,
            "schema": {
              "type": "number",
              "default": 0.1,
              "title": "Box Threshold"
            },
            "description": "Minimum confidence score to keep a detected bounding box. Lower values return more boxes but may include noise."
          },
          {
            "name": "group_lines",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": true,
              "title": "Group Lines"
            },
            "description": "Group detected words into lines based on spatial proximity."
          },
          {
            "name": "group_blocks",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false,
              "title": "Group Blocks"
            },
            "description": "Group detected lines into blocks based on spatial proximity."
          },
          {
            "name": "paragraph_break",
            "in": "query",
            "required": false,
            "schema": {
              "type": "number",
              "default": 0.0035,
              "title": "Paragraph Break"
            },
            "description": "Normalized vertical distance threshold for splitting text into separate blocks. Only used when group_blocks is enabled."
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_perform_ocr_ocr__post"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/OCROut"
                  },
                  "title": "Response Perform Ocr Ocr  Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
